b;d="https://twitter.com/intent/tweet";a=d+"?"+b+"&"+a+"&via=JornalExtra&"+e;break;case "gplus":d="https://plus.google.com/share";a=d+"?url="+b}c&&a&&dispatchPopUp(a,c.capitalize());return false});$(".social_tools").off("click").on("click",".whatsapp a",function(){var c=$(this).parents("ul").attr("data-id");c=/\.html/.test(window.location.href)?encodeURIComponent(EncurtadorInfoglobo.encurtarArtigo(c,"whatsapp")):encodeURIComponent(EncurtadorInfoglobo.encurtarSecao(c,"whatsapp"));var b=$("#stage .article h1").text();
this.href="whatsapp://send?"+("text="+(b.length?b.replace(/\#/g,""):truncateString(document.title,70)))+" - "+c})});function dispatchPopUp(c,b){var a={Twitter:{width:600,height:450},Facebook:{width:690,height:600},Gplus:{width:500,height:500}};if(!a[b])return false;window.open(c,"_blank","width="+a[b].width+", height="+a[b].height+", top="+(screen.availHeight/2-a[b].height/2)+", left="+(screen.availWidth/2-a[b].width/2),false)}
function networkFromClassName(c){if(/\b(facebook|twitter|gplus)\b/i.test(c))return RegExp.$1.toLowerCase();return""}String.prototype.capitalize=function(){return this.replace(/(?:^|\s)\S/g,function(c){return c.toUpperCase()})};
function checkOldBrowser(){this.isOldBrowser=false;this.browserName="Desconhecido";this.browserVersion="";this.browserMainVersion=0;this.oldBrowsers={ie:{name:"Microsoft Internet Explorer",re:/\bMSIE ((\d+)([\.\d]+)?)/,versions:"<=8"},chrome:{name:"Google Chrome",re:/\bChrome\/((\d+)([\.\d]+)?)/},firefox:{name:"Mozilla Firefox",re:/\bFirefox\/((\d+)([\.\da-z]+)?)/},opera:{name:"Opera",re:/\bOpera[ \/]((\d+)([\.\d]+)?)/},safariMac:{name:"Safari (Macintosh)",re:/\bMacintosh.+Version\/((\d+)([\.\d]+)?).+\bSafari\b/},
safariWin:{name:"Safari (Windows)",re:/\bWindows.+Version\/((\d+)([\.\d]+)?).+\bSafari\b/}};this.source=navigator.userAgent||navigator.vendor||window.opera;this.checkBrowser=function(){for(var a in this.oldBrowsers)if(this.oldBrowsers[a].re.test(this.source)){this.browserName=this.oldBrowsers[a].name;this.browserVersion=RegExp.$1;this.browserMainVersion=parseInt(RegExp.$2);this.browserId=a;if(this.checkVersion())this.isOldBrowser=true;break}return this.isOldBrowser};this.checkVersion=function(){var a=
this.oldBrowsers[this.browserId].versions||false,b=this.browserMainVersion,c=function(){return false};if(a===false)return false;if(/^\s*\d+(\s*,\s*\d+)*\s*$/.test(a)){var f=a.replace(/^\s+|\s+$/g,"").split(/\s*,\s*/);c=function(){for(var e=0;e<f.length;e++)if(b==f[e])return true;return false}}else if(/^\s*(<=?|>=?)(\d+)\s*$/.test(a)){a=RegExp.$1;var d=parseInt(RegExp.$2);switch(a){case "<":c=function(){return b<d};break;case "<=":c=function(){return b<=d};break;case ">":c=function(){return b>d};break;
case ">=":c=function(){return b>=d}}}return c()}}
function oldBrowserNotice(){this.checkOldBrowser=new checkOldBrowser;this.title="Seu navegador est&aacute; desatualizado!";this.message="Clique no &iacute;cone correspondente para atualizar ou escolha outro navegador";this.suggested={chrome:{image:"chrome.png",downloadLink:"https://www.google.com/chrome?hl=pt",title:"Google Chrome"},firefox:{image:"firefox.png",downloadLink:"https://www.mozilla.org/pt-BR/firefox/fx/",title:"Mozilla Firefox"},msie:{image:"msie.png",downloadLink:"https://windows.microsoft.com/pt-br/internet-explorer/download-ie",
title:"Microsoft Internet Explorer"},safari:{image:"safari.png",downloadLink:"https://www.apple.com/safari/",title:"Apple Safari"},opera:{image:"opera.png",downloadLink:"https://www.opera.com/pt/",title:"Browser Opera"}};this.articleLink=this.articleLinkLabel="";this.init=function(){this.mainSuggestion="chrome";this.checkOldBrowser.checkBrowser();if(this.checkOldBrowser.isOldBrowser){this.mainSuggestion=this.checkOldBrowser.browserId;this.bar=$(".old-browser-bar");if(this.bar.length==0)this.bar=$('<div class="old-browser-bar" />').appendTo("body");
this.bar.css({display:"none",position:"fixed",top:0,left:0,width:"100%",zIndex:topZ()}).html(this.barHTML()).slideDown(400);$("body").animate({marginTop:"80px"},400)}$(".old-browser-bar .close").off("click").on("click",function(){$(this).parents(".old-browser-bar").slideUp(400,function(){$(this).remove()});$("body").animate({marginTop:"0px"},400)})};this.barHTML=function(){var a='<div class="inner"><a class="close" href="javascript://"></a><div class="title"><h2>'+this.title+"</h2><small>"+this.message+
"</small></div><ul>",b;for(b in this.suggested)a+='<li class="browser'+(b==this.mainSuggestion?" main":"")+'"><a href="'+this.suggested[b].downloadLink+'" class="'+b+'" title="'+this.suggested[b].title+'"></a></li>';a+="</ul>";if(this.articleLink&&this.articleLinkLabel)a+='<a class="article-link" href="'+this.articleLink+'">'+this.articleLinkLabel+"</a>";a+="</div>";return a}}
$(function(){if(getCookie("oldBrowserBarViewed")!="yes"){var a=new oldBrowserNotice;a.checkOldBrowser.oldBrowsers.ie.versions="<=8";a.checkOldBrowser.oldBrowsers.firefox.versions="<=14";a.checkOldBrowser.oldBrowsers.chrome.versions="<=18";a.articleLinkLabel="Entenda os benef\ufffdcios";a.init();setCookie("oldBrowserBarViewed","yes",1)}});
