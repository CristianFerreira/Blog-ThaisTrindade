return false}})}function fecharListaComentario(){$("#comentario_Form > header").on("click","a.close",function(){$(this).parents("section").slideUp();$("section.box.comments, section.box.social.new-comments").slideDown(500,function(){$(this).find(".comment_arrow").show()});atualizarBoxComentarios();return false})}
