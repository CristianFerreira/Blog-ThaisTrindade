function ocultarBoxLateral(){$("section.box.comments, section.box.social.new-comments").slideUp().find(".comment_arrow").hide()}
function exibirListaComentario(a){var c=$("#comentario_Form");carregarFormulario();switch(a){case 1:c.slideDown(500,function(){c.find(".user_input li:first-child").length&&$("html, body").animate({scrollTop:c.find(".user_input li:first-child").offset().top},500)});break;case 2:c.slideDown(500,function(){$("html, body").animate({scrollTop:0},500)});break;default:c.slideDown()}usuarioLogado()}
function usuarioLogado(a){if(!isComentarioEncerrado&&isUserLogged()){$("#commentFormResposta,#commentDisclaimerResposta,#commentFormComentario,#commentDisclaimerComentario").show();if($('input:hidden[id="isApelidoDesligado"]').val()!="true"&&objUser.communityUser.apelido.length>0){$('input:hidden[name="nome"]').val(objUser.communityUser.apelido);$(".name.apelido").find("strong").html(objUser.communityUser.apelido);if($(".name.apelido #alterarApelidoResposta").length){$(".name.apelido #alterarApelidoResposta").attr("href",
$(".name.apelido #alterarApelidoResposta").attr("href").substring(0,$(".name.apelido #alterarApelidoResposta").attr("href").lastIndexOf("="))+"="+$(".name.apelido").find("strong").html());var c=$(".name.apelido #alterarApelidoResposta").attr("href").split("responder=");$(".name.apelido #alterarApelidoResposta").attr("href",c[0]+"responder="+a+c[1])}else $(".name.apelido #alterarApelido").attr("href",$(".name.apelido #alterarApelido").attr("href").substring(0,$(".name.apelido #alterarApelido").attr("href").lastIndexOf("="))+
"="+$(".name.apelido").find("strong").html())}else if(!isComentarioEncerrado&&$('input:hidden[id="isApelidoDesligado"]').val()!="true"&&objUser.communityUser.apelido.length==0){$(".name.apelido").hide();$(".name.nome").show();$(".name.nome").find("span a").html("cadastrar apelido");$(".name.nome").find("span a").attr("href",getUrl()+"?service=dadosComplementares&urlRetorno="+getUrlCompleta()+"?exibirFormulario=true");$(".name.nome").find("strong").html(objUser.cadun.nome);$('input:hidden[name="nome"]').val(objUser.cadun.nome)}else{$('input:hidden[name="nome"]').val(objUser.cadun.nome);
$(".name.nome").find("strong").html(objUser.cadun.nome)}}else $("#commentLoginWarningResposta,#commentLoginWarningComentario").show()}
