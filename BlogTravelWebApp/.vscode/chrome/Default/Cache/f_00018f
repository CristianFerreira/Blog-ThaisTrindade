function ativarFormulario(){$("#comentario_Form form, #modalDenuncia form").live("submit",function(){processarFormulario($(this));return false})}
function processarFormulario(a){if(validarFormComentario(a)){var c=a.attr("action");$.ajax({dataType:"json",type:"post",contentType:"application/x-www-form-urlencoded; charset=UTF-8",url:c,data:a.serialize(),beforeSend:function(){a.find(".buttons").append('<div class="loader_ajax"></div>').find("input").addClass("disabled").attr("disabled","disabled");a.find(".terms").hide()},complete:function(){a.find(".buttons").find("input").removeClass("disabled").removeAttr("disabled");a.find(".loader_ajax").remove();
a.find(".terms").show()},success:function(b){limparFormulario(a);if(b.erros){$.each(b.erros,function(f,g){a.find("[name="+f+"]").parent().remove("error");a.find("[name="+f+"]").parent().addClass("error").append('<span class="validation info">'+g+"</span>")});a.find("li:not(.error, .success)").each(function(f,g){$(g).addClass("success").append('<span class="validation info"></span>')})}else if(b.isDenuncia){b=$("#modalDenuncia");b.css({position:"relative"});var d=b.find(".wrapper blockquote");d.outerWidth();
d=d.outerHeight();b.find(".response");b.find(".response h4").css({margin:0,lineHeight:d+"px"});b.find(".wrapper").hide();b.find(".response").show();b.find(".response").addClass("success");setTimeout(closeXModal,2E3);salvarCookieComentario(a)}else if(b.erroInterno!=null)mensagemComentario(a,"alert",b.erroInterno);else if(b.sessaoExpirada!=null)mensagemComentario(a,"alert",b.sessaoExpirada);else if(b.stopWords!=null)mensagemComentario(a,"alert",b.stopWords);else if(b.isModerado){a.find("[name=body], [name=captcha]").val("");
mensagemComentario(a,"moderating","Seu coment&aacute;rio foi enviado com sucesso e ser&aacute; analisado para publica&ccedil;&atilde;o.");if(a.parents(".user_input").length>0){a.find("fieldset").fadeOut(800);a.delay(4E3).slideUp()}salvarCookieComentario(a)}else{d=$("#comentario_Form");a.find("[name=body], [name=captcha]").val("");mensagemComentario(a,"moderating","Seu coment&aacute;rio foi publicado com sucesso.");d.find(".user_input ul").length==0&&d.find(".user_input").append("<ul>");d.find(".user_input h3").length==
0&&d.find(".user_input").prepend("<h3>");var e=b.qtdTotal+" coment\u00e1rio";if(b.qtdTotal>1)e+="s";d.find(".user_input h3").text(e);$(".toolbar_bottom .comment").text("Comente ("+b.qtdTotal+")");$(".comments_count").text(b.qtdTotal);d.find(".user_input ul li").removeClass("highlight");if(typeof b.idNovoComentario!=="undefined")jQuery.data(document.body,"paginacao")||jQuery(document.body).data("paginacao",{idNovoComentario:b.idNovoComentario});if(isTouchDevice()==false)if(a.parents(".user_input").length>
0){a.parent().find("ul.reply").length==0&&a.after('<ul class="reply"></ul>');d=a.parent().find("ul.reply");d.prepend(innerShiv(getTemplate("extraComentario/infoglobo/ejs/listaRespostas",b),false));d.find("li:first").addClass("highlight");d.find("li:lt("+b.comentarios.length+")");$("#commentDisclaimerResposta").slideUp();a.slideUp();removeComentarioDuplicado("ul.reply")}else{d.find(".user_input > ul").prepend(innerShiv(getTemplate("extraComentario/infoglobo/ejs/lista",b),false));d.find(".user_input > ul li:first").addClass("highlight");
d.find(".user_input > ul > li:lt("+b.comentarios.length+")");removeComentarioDuplicado(".user_input > ul")}else closeExtraModal();salvarCookieComentario(a);typeof window.getVotes=="function"&&typeof window.ratingNosComentariosDesligado=="undefined"&&getVotes()}},error:function(){mensagemComentario(a,"alert","Ops! Ocorreu um erro inesperado. Tente novamente.");atualizarCaptcha()}})}}
function removeComentarioDuplicado(a){var c={};$(a).find("input").each(function(){var b=$(this).val();if(c[b])$(this).parent().remove();else c[b]=true})}function mensagemComentario(a,c,b){a.find("> ul").remove();a.prepend('<ul><li class="highlight '+c+'"><h4>'+b+"</h4></li></ul>")}
function carregarResposta(){$("body").on("click","#comentario_Form .reply_this",function(a){a.preventDefault();var c=$(this);idResposta=c.attr("href").substring(c.attr("href").indexOf("parentId=")).split("=")[1];var b=c.parents("li").find("form");a=c.parents("li").find(".login_warning");if(b.length==0&&a.length==0){a=c.parents("li").find("> [name=idComentario]").val();if(c.parents("li").find("> ul.reply li").length)a=c.parents("li").find("> ul.reply li:first > [name=idComentario]").val();$.ajax({async:false,
type:"post",contentType:"application/x-www-form-urlencoded; charset=UTF-8",url:c.attr("href")+"&primeiroComentario="+a,beforeSend:function(){},complete:function(){},success:function(d){$("#comentario_Form .user_input #commentLoginWarningResposta").remove();$("#comentario_Form .user_input #commentDisclaimerResposta").remove();$("#comentario_Form .user_input form#commentFormResposta").remove();b=$(d).hide();c.parent().parent().after(b);carregarCookieComentario();gerarCaptcha();usuarioLogado(idResposta);
$("#commentLoginWarningResposta").is(":visible")&&$("#commentLoginWarningResposta a").attr("href","javascript:redirectToLogin('responder="+idResposta+"');")}})}else{$("#comentario_Form .user_input #commentLoginWarningResposta").remove();$("#comentario_Form .user_input #commentDisclaimerResposta").remove();$("#comentario_Form .user_input form#commentFormResposta").remove()}return false})}
function salvarCookieComentario(a){a.find('input:text[name="field(byline)"]').length&&setCookie("autorComentario",a.find('input:text[name="field(byline)"]').val(),null);a.find('input:text[name="email"]').length&&setCookie("emailComentario",a.find('input:text[name="email"]').val(),null);carregarCookieComentario();return true}
function carregarCookieComentario(){getCookie("emailComentario")!=""&&getCookie("autorComentario")!=""&&$.each($("#comentario_Form form, #modalDenuncia form"),function(a,c){$(c).find('input:text[name="email"]').val(getCookie("emailComentario"));$(c).find('input:text[name="field(byline)"]').val(getCookie("autorComentario"))})}
function denunciarComentario(){$("body").on("click","section.comments .report_abuse, section.new-comments .report_abuse",function(a){a.preventDefault(a);var c=$(this),b=getParamByName("parentId",c.attr("href"));if(localStorage.getItem("denunciado-"+b))msgOverElement($(c).parent(),'<div class="message"><p>Voc\u00ea j\u00e1 denunciou esse coment\u00e1rio</div>');else{if(typeof window.denunciaComCadastro!="undefined"&&denunciaComCadastro&&!isLogadoGloboCom()&&!isLogadoCadun()){redirectToLogin("denunciar="+
b);return false}if($(this).is("aside *")){a=$(this).siblings("span.info").html().split(/<time/)[0];var d=$(this).siblings("span.info").find("time").text(),e=$(this).siblings("p").find("a").text()}else{d=$(this).parent().parent().siblings(".avatar");a=d.find("span").text();d=d.find("time").text();e=$(this).parent().parent().html().split(/<div class="functions">/)[0]}var f=e+"<span><strong>"+a+"</strong> "+d+"</span>";$.ajax({async:false,type:"post",contentType:"application/x-www-form-urlencoded; charset=UTF-8",
