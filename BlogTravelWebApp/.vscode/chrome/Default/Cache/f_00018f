e=b?"":"<a class=\"button default medium add-comment\" onclick=\"_gaq.push(['_trackEvent', 'Coment\u00e1rios', 'Abrir box coment\u00e1rio', '/${secaoPai}${section.name}/${article.articleTypeName}/Box_Comentarios/']);\" class=\"button default medium add-comment\" href=\"javascript: void(0);\">Comentar</a>";e=e+'<a class="button default medium read-comments" href="javascript: void(0);" onclick="_gaq.push([\'_trackEvent\', \'Coment\u00e1rios\', \'Ler Todos\', \'/${secaoPai}${section.name}/${article.articleTypeName}/Box_Comentarios/\']);">Ler todos</a><div class="clearfix"></div><div style="margin:10px 0 0; font-size:11px; text-align:center;">Voc\u00ea \u00e9 respons\u00e1vel pela sua opini\u00e3o</div>';
a.find(".participate.leia_mais").html(e)}a.find(".body").prepend(innerShiv(getTemplate("extraComentario/infoglobo/ejs/box",d),false));typeof window.boxGetVotes=="function"&&typeof window.ratingNosComentariosDesligado=="undefined"&&boxGetVotes();a.find(".body").prepend(a.find(".comment_term"));a.find(".participate.encerrado").length>0&&a.find(".body").prepend(a.find(".participate.encerrado"));if(a.find(".quote .number").length==0&&d.totalComentarios>0)$("body.mulher").length?a.find(".quote").prepend('<span class="number"></span>'):
