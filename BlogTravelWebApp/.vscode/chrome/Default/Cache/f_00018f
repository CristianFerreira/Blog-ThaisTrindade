function validarFormComentario(a){if($("#denunciaComCadastro").val())var c={"field(byline)":{obrigatorio:true,tamanhoMin:2},email:{obrigatorio:false,email:false},body:{obrigatorio:true,tamanhoMin:2},captcha:{obrigatorio:true}},b={"field(byline)":{obrigatorio:"Digite o seu nome",tamanhoMin:"Nome inv&aacute;lido"},email:{obrigatorio:"Digite o seu e-mail",email:"Email Inv&aacute;lido"},body:{obrigatorio:"Texto inv&aacute;lido",tamanhoMin:"Texto inv&aacute;lido"},captcha:{obrigatorio:"Digite o texto da imagem"}};
else{c={"field(byline)":{obrigatorio:true,tamanhoMin:2},email:{obrigatorio:true,email:true},body:{obrigatorio:true,tamanhoMin:2},captcha:{obrigatorio:true}};b={"field(byline)":{obrigatorio:"Digite o seu nome",tamanhoMin:"Nome inv&aacute;lido"},email:{obrigatorio:"Digite o seu e-mail",email:"Email Inv&aacute;lido"},body:{obrigatorio:"Texto inv&aacute;lido",tamanhoMin:"Texto inv&aacute;lido"},captcha:{obrigatorio:"Digite o texto da imagem"}}}return validarForm(a,c,b)}
