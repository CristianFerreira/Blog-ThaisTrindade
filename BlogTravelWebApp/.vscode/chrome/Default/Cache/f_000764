var PostsComponent = (function () {
    function PostsComponent(postDataService, autenticationService, router, snackBar, route, userLoggedService, sanitizer) {
        this.postDataService = postDataService;
        this.autenticationService = autenticationService;
        this.router = router;
        this.snackBar = snackBar;
        this.route = route;
        this.userLoggedService = userLoggedService;
        this.sanitizer = sanitizer;
        this.posts = new Array();
        this.postsRender = new Array();
        this.pageSize = 2;
        this.currentPage = 1;
        this.pageSizeOptions = [5, 10, 15];
        // this.mdPaginatorIntl.itemsPerPageLabel = 'Art√≠culos por p√°gina:';
        // this.mdPaginatorIntl.nextPageLabel = 'Siguiente p√°gina';
        // this.mdPaginatorIntl.previousPageLabel = 'Pagina anterior';
    }
    PostsComponent.prototype.ngOnInit = function () {
        var _this = this;
        this.userLoggedService.logged.subscribe(function (logged) {
            _this.logged = logged;
        });
        this.updateUserLogged();
        this.route.params.subscribe(function (params) {
            _this.idPost = params['id'];
            var tag = params['tag'];
            var category = params['category'];
            var destiny = params['destiny'];
            if (_this.idPost != null)
                _this.getById(_this.idPost.toString());
            else if (tag != null)
                _this.getByTag(tag);
            else if (category != null)
                _this.getByCategory(category);
            else if (destiny != null)
                _this.getByDestiny(destiny);
            else
                _this.getAll();
        });
        // this.disqus();
    };
    PostsComponent.prototype.postsToRender = function (topo) {
        this.backToTop(topo);
        var begin = ((this.currentPage - 1) * this.pageSize);
        var end = begin + this.pageSize;
        this.postsRender = this.posts.slice(begin, end);
    };
    PostsComponent.prototype.updateTablePosts = function (pageEvent) {
        this.currentPage = (pageEvent.pageIndex + 1);
        this.pageSize = pageEvent.pageSize;
        this.postsToRender();
    };
    PostsComponent.prototype.backToTop = function (topo) {
        $('html, body').animate({
            scrollTop: topo ? 0 : ($('#post_id').position().top - 100)
        }, 500);
    };
    PostsComponent.prototype.disqus = function () {
        (function () {
            var d = document, s = d.createElement('script');
            s.src = 'https://thaistrindade.disqus.com/embed.js';
            s.setAttribute('data-timestamp', '' + new Date());
            (d.head || d.body).appendChild(s);
        })();
    };
    PostsComponent.prototype.updateUserLogged = function () {
        var logged = (this.autenticationService.contextoLogado() != null);
        this.userLoggedService.userLogged(logged);
    };
    PostsComponent.prototype.postURL = function (id) {
        return this.sanitizer.bypassSecurityTrustUrl("https://www.facebook.com/plugins/share_button.php?href=http%3A%2F%2Fwww.thaistrindade.com%2F%23%2Fpost%2F" + id + "&layout=button&size=small&mobile_iframe=true&width=97&height=20&appId");
    };
    PostsComponent.prototype.getAll = function () {
        var _this = this;
        this.postDataService.getAll().subscribe(function (result) {
            _this.posts = result.json();
            _this.postsToRender(true);
        }, function (error) {
        });
    };
    PostsComponent.prototype.getByTag = function (tag) {
        var _this = this;
        this.postDataService.getByTag(tag).subscribe(function (result) {
            _this.posts = result.json().data;
            _this.postsToRender();
        }, function (error) {
        });
    };
    PostsComponent.prototype.getByCategory = function (category) {
        var _this = this;
        this.postDataService.getByCategory(category).subscribe(function (result) {
            _this.posts = result.json().data;
            _this.postsToRender();
        }, function (error) {
        });
    };
    PostsComponent.prototype.getByDestiny = function (destiny) {
        var _this = this;
        this.postDataService.getByContinent(destiny).subscribe(function (result) {
            _this.posts = result.json().data;
            _this.postsToRender();
        }, function (error) {
        });
    };
    PostsComponent.prototype.getById = function (id) {
        var _this = this;
        this.postDataService.getById(id).subscribe(function (result) {
            var post = result.json().data;
            _this.posts.push(post);
            _this.postsToRender();
        }, function (error) {
        });
    };
    PostsComponent.prototype.pagePostTag = function (tag) {
        this.router.navigateByUrl('post/tags/' + tag);
    };
    PostsComponent.prototype.pageEditPost = function (id) {
        this.router.navigateByUrl('post-create/' + id);
    };
    PostsComponent.prototype.openSnackBarDelete = function (id) {
        var _this = this;
        var snackBarRef = this.snackBar.open("Excluir postagem ?", "Sim", {
            duration: 3000,
        });
        snackBarRef.onAction().subscribe(function () {
            _this.delete(id);
        });
    };
    PostsComponent.prototype.delete = function (id) {
        var _this = this;
        this.postDataService.deleteById(id).subscribe(function (result) {
            var post = _this.posts.filter(function (post) { return post._id == id; })[0];
            var index = _this.posts.indexOf(post);
            if (index > -1) {
                _this.posts.splice(index, 1);
                var snackBarRef = _this.snackBar.open("Postagem excluida com sucesso", "OK", {
                    duration: 3000,
                });
            }
        });
    };
    return PostsComponent;
