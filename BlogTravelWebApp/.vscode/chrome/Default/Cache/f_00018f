var validaFaleComExtra={nameFrom:{test:/[a-z]+/i,minSize:3,msg:{test:"Digite seu nome",minSize:"Min. 3 caracteres"}},emailFrom:{test:function(a){return/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i.test(a)},msg:{test:"Email inv\u00e1lido",empty:"Digite seu email"}},assunto:{test:function(a){return a!="selecione"},msg:{test:"Escolha o assunto"}},message:{test:/[a-z]+/i,minSize:5,maxSize:200,msg:{test:"Digite sua mensagem",minSize:"Min. 5 caracteres",maxSize:"Max. 200 caracteres"}},
captcha:{test:/^[a-z0-9]+$/i,msg:{test:"Digite o texto da imagem"}},cpf:{test:validarCPF,msg:{test:"CPF inv\u00e1lido",empty:"Digite o seu CPF"}},telefone:{test:/^(\(\d{2}\)\s\d{4,5}\-\d{4})?$/,msg:{test:"N\u00famero inv\u00e1lido"}}};
function formValidation(a,f){var d={};$(":input",a).each(function(g,c){var e=$(c).val(),b=f[c.name]||false;if(!b)return true;if(b.msg.empty&&!e)d[c.name]=b.msg.empty;if(typeof b.test=="function"&&!b.test(e)||b.test instanceof RegExp&&!b.test.test(e))d[c.name]=b.msg.test;if(b.minSize&&e.length<b.minSize)d[c.name]=b.msg.minSize||b.msg.test;if(b.maxSize&&e.length>b.maxSize)d[c.name]=b.msg.maxSize||b.msg.test});return d}
function resetValidatedFormFields(a){$(":input",a).removeClass("success error");$("span.validation",a).remove()}function markValidatedFormFields(a,f){$(":input",a).each(function(d,g){var c=f[this.name]||false;c?$(g).addClass("error").after('<span class="validation error">'+c+"</span>"):$(g).addClass("success").after('<span class="validation success" />')})}
function validarCPF(a){a=a.replace(/[^\d]+/g,"");if(a=="")return false;if(a.length!=11||a=="00000000000"||a=="11111111111"||a=="22222222222"||a=="33333333333"||a=="44444444444"||a=="55555555555"||a=="66666666666"||a=="77777777777"||a=="88888888888"||a=="99999999999")return false;for(i=add=0;i<9;i++)add+=parseInt(a.charAt(i))*(10-i);rev=11-add%11;if(rev==10||rev==11)rev=0;if(rev!=parseInt(a.charAt(9)))return false;for(i=add=0;i<10;i++)add+=parseInt(a.charAt(i))*(11-i);rev=11-add%11;if(rev==10||rev==
11)rev=0;if(rev!=parseInt(a.charAt(10)))return false;return true};
