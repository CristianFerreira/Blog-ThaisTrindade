function NewWindowGlobo(a,b,c,d,e,f){if(b_versao>=4){LeftPosition=screen.width?(screen.width-c)/2:0;TopPosition=screen.height?(screen.height-d)/2:0}else TopPosition=LeftPosition=100;a="http://oglobo2.globo.com"+a;a=window.open(a,b,"height="+d+",width="+c+",top="+TopPosition+",left="+LeftPosition+",scrollbars="+e+",resizable="+f+"");a.window.focus&&a.window.focus()}function bumper_redirect(a){document.location=a}
function bn_findPos(a){var b=curtop=0;if(a.offsetParent){b=a.offsetLeft;for(curtop=a.offsetTop;a=a.offsetParent;){b+=a.offsetLeft;curtop+=a.offsetTop}}return[b,curtop]}function valida_email(a){var b=a.length-1,c=0;for(i=0;i<a.length;i++){a.charAt(i)=="@"&&c++;if(c>1||a.charAt(i)=="/")return false}if(a.charAt(0)=="@"||a.charAt(0)=="."||a.charAt(b)=="@"||a.charAt(b)=="."||a.indexOf("@")==-1||a.indexOf(".")==-1||a.indexOf("@.")!=-1)return false;return true}
function validaForm(){var a=document.frmEmail;if(a.email.value==""||a.nome.value==""||a.assunto.value=="selecione"){window.alert("Os campos nome, e-mail e assunto s\u00e3o de preenchimento obrigat\u00f3rio!");a.nome.focus();return false}if(!valida_email(a.email.value)){window.alert("Por favor, informe um e-Mail v\u00e1lido.");a.email.focus();return false}if(a.txtTexto.value==""){window.alert("Digite o texto !");a.txtTexto.focus();return false}return true};
function isEmail(a){return a.match("^[\\w-]+(\\.[\\w-]+)*@([\\w-]+\\.)+[a-zA-Z]{2,3}$")}
function validarCPF(a){a=a.replace(/[^\d]+/g,"");if(a=="")return false;if(a.length!=11||a=="00000000000"||a=="11111111111"||a=="22222222222"||a=="33333333333"||a=="44444444444"||a=="55555555555"||a=="66666666666"||a=="77777777777"||a=="88888888888"||a=="99999999999")return false;for(i=add=0;i<9;i++)add+=parseInt(a.charAt(i))*(10-i);rev=11-add%11;if(rev==10||rev==11)rev=0;if(rev!=parseInt(a.charAt(9)))return false;for(i=add=0;i<10;i++)add+=parseInt(a.charAt(i))*(11-i);rev=11-add%11;if(rev==10||rev==
11)rev=0;if(rev!=parseInt(a.charAt(10)))return false;return true}function validarEmail(a){return/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i.test(a)}
function validarArquivo(a){extensoes_permitidas=[".mp3",".mp4",".avi",".wav",".wma",".wmv",".mov",".vob",".rm",".mpeg",".mpg",".txt",".doc",".pdf",".xls",".rtf",".jpg",".gif",".flv",".jpeg"];permitida=false;if(a){extensao=a.substring(a.lastIndexOf(".")).toLowerCase();if($.inArray(extensao,extensoes_permitidas)>-1)permitida=true}else return permitida;return permitida}function validarCombos(a){permitida=true;if(a=="selecione")permitida=false;return permitida}
function validarData(a){permitida=false;if(/^\d{2}\/\d{2}\/\d{4}$/.test(a))permitida=true;return permitida}function limparFormulario(a,h){a.find(".highlight").parent().remove();a.find("li").removeClass("error success").find("span.validation").remove();h&&a[0].reset()}
function validarForm(a,h,e){var g=true;$(a).find("input[type='text'], textarea").each(function(k,j){var b=$(j),d=h[b.attr("name")],c=e[b.attr("name")];if(d&&c){var f;if(d.obrigatorio&&c.obrigatorio&&b.val().trim()=="")f=c.obrigatorio;else if(d.tamanhoMin&&c.tamanhoMin&&b.val().trim().length<d.tamanhoMin)f=c.tamanhoMin;else if(d.tamanhoMax&&c.tamanhoMax&&b.val().trim().length>d.tamanhoMax)f=c.tamanhoMax;else if(d.email&&c.email&&!isEmail(b.val().trim()))f=c.email;else if(d.cpf&&c.cpf&&!validarCPF(b.val().trim()))f=
c.cpf;b=b.parent("li");b.removeClass("error success").find("span:not(label span)").remove();if(f){b.addClass("error").append('<span class="validation info">'+f+"</span>");g=false}else b.addClass("success").append('<span class="validation info"></span>')}});return g}function gerarCaptcha(){var a=$(".captcha_img");a.find("img").length&&a.find("img").remove();a.prepend('<img src="'+getUrl()+"escenicCaptcha?token="+window.tokenTimeJS+'" alt="captcha" width="75" height="37">')}
function atualizarCaptcha(){var a=(new Date).getTime();$(".captcha_img").find("img").each(function(h,e){e=$(e);var g=e.attr("src");g=g.replace(/([0-9])+$/,a);e.attr("src",g);e.closest(".captcha_img").siblings("#captcha").val("").focus()})};
