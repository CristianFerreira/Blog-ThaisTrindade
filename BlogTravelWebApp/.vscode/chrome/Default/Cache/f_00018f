applyVotes(votesCache)}})}
function abrirListaComentario(){var a=$("section.box.comments"),c=$("section.box.social.new-comments");a.on("click","ul > li > p > a",function(b){b.preventDefault();ocultarBoxLateral();exibirListaComentario(1);return false});a.on("click",".button.default.medium",function(b){b.preventDefault();ocultarBoxLateral();exibirListaComentario();return false});c.on("click","ul > li > p > a",function(b){b.preventDefault();ocultarBoxLateral();exibirListaComentario(1);return false});c.on("click",".button.default.medium",
function(b){b.preventDefault();ocultarBoxLateral();exibirListaComentario();return false});a.find("a.button.default.medium.add-comment").click(function(){ocultarBoxLateral();exibirListaComentario();return false});c.on("click",".button_comment a",function(){$(this).text();ocultarBoxLateral();exibirListaComentario();return false});a.find("a.button.default.medium.read-comments").click(function(){ocultarBoxLateral();exibirListaComentario(1);return false});a.find("header h2 a").live("click",function(){ocultarBoxLateral();
exibirListaComentario(1);return false});$(".social_tools .comment a").bind("click",function(){ocultarBoxLateral();exibirListaComentario(2);return false});$(".blog.multimedia.news.editorial .fotopost .toolbar_bottom a.comment").bind("click",function(){exibirListaComentario();$("section.comments > div.new_comment a").parent().remove();return false});$(".toolbar_bottom a.comment").not(".fotopost a").bind("click",function(){ocultarBoxLateral();exibirListaComentario(2);return false});$(".single_post .toolbar_bottom a.comment").bind("click",
function(){ocultarBoxLateral();exibirListaComentario(2);return false});$(".single_post .comments_count a").bind("click",function(){ocultarBoxLateral();exibirListaComentario(2);return false});$(".fotopost .comments_count a").bind("click",function(){$("section.comments > form").slideDown(500,function(){$("html, body").animate({scrollTop:$(this).offset().top},500)});$("section.comments > div.new_comment a").parent().remove();return false})}
